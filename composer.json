{
    "name": "adishardiseffectris/messenger",
    "description": "Laravel messenger suite.",
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Tippin / Adam Hardarson",
            "email": "adam.hardarson@effectris.se",
            "homepage": "https://effectris.se"
        }
    ],
    "homepage": "https://github.com/adishardiseffectris/messenger",
    "keywords": [
        "Laravel",
        "Messenger"
    ],
    "require": {
        "php": "^7.4||^8.0",
        "ext-json": "*",
        "laravel/framework": "^8.42",
        "joypixels/emoji-toolkit": "^6.0",
        "intervention/image": "^2.5",
        "intervention/imagecache": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "AdisHardisEffectris\\Messenger\\": "src/",
            "AdisHardisEffectris\\Messenger\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdisHardisEffectris\\Messenger\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AdisHardisEffectris\\Messenger\\MessengerServiceProvider"
            ],
            "aliases": {
                "Messenger": "RTippin\\Messenger\\Facades\\Messenger"
            }
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
